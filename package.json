{
  "name": "api-express",
  "version": "1.0.0",
  "description": "api restful with typescript",
  "main": "index.js",
  "scripts": {
    "start": "SET NODE_ENV=production&& tsc && node dist/server.js",
    "start:dev": "SET NODE_ENV=dev&& nodemon --exec ts-node ./src/server.ts",
    "start:prod": "SET NODE_ENV=production&& npm start",
    "typeorm": "typeorm-ts-node-esm -d ./src/config/data.source.ts",
    "m:gen": "npm run typeorm migration:generate",
    "m:run": "npm run typeorm migration:run"
  },
  "author": "maicol arcila",
  "license": "ISC",
  "dependencies": {
    "@types/passport": "^1.0.12",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "passport": "^0.6.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.15",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  }
}
